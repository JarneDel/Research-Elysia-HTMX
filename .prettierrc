{
  "$schema": "https://json.schemastore.org/prettierrc",
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "bracketSpacing": true,
  "printWidth": 80,
  "semi": false,
  "singleQuote": true,
  "tabWidth": 2,
  "trailingComma": "all",
  "useTabs": false,
  "htmlWhitespaceSensitivity": "css",
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "",
    "<THIRD_PARTY_MODULES>",
    "<BUILT_IN_MODULES>",
    "^@/",
    "^~/",
    "^[.]"
  ]
}
